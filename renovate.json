{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "enabled": true,
  "separateMajorMinor": true,
  "packageRules": [
    {
      "matchPackagePatterns": [
        "com.soebes.itf.jupiter.extension.*"
      ],
      "groupName": "all own dependencies",
      "groupSlug": "all-own-dependencies"
    }
  ]
}